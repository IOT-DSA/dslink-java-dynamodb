{
  "name": "dslink-java-dynamodb",
  "version": "0.0.2",
  "description": "Historian DSLink implementation for DynamoDB",
  "license": "Apache",
  "author": {
    "name": "Daniel Shapiro"
  },
  "main": "bin/dslink-java-dynamodb",
  "repository": {
    "type": "git",
    "url": "https://github.com/IOT-DSA/dslink-java-dynamodb"
  },
  "bugs": {
    "url": "https://github.com/IOT-DSA/dslink-java-dynamodb/issues"
  },
  "configs": {
    "name": {
      "type": "string",
      "default": "dynamodb"
    },
    "broker": {
      "type": "url"
    },
    "token": {
      "type": "string"
    },
    "nodes": {
      "type": "path",
      "default": "nodes.json"
    },
    "key": {
      "type": "path",
      "default": ".key"
    },
    "log": {
      "type": "enum",
      "default": "info"
    },
    "handler_class": {
      "type": "string",
      "default": "org.iot.dsa.dynamodb.Main"
    }
  }
}
